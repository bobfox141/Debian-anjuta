# _LT_SYS_HIDDEN_LIBDEPS([TAGNAME])
# ---------------------------------
# Figure out "hidden" library dependencies from verbose
# compiler output when linking a shared library.
# Parse the compiler output and extract the necessary
# objects, libraries and library flags.
m4_defun([_LT_SYS_HIDDEN_LIBDEPS],
[m4_require([_LT_FILEUTILS_DEFAULTS])dnl
# Dependencies to place before and after the object being linked:
_LT_TAGVAR(predep_objects, $1)=
_LT_TAGVAR(postdep_objects, $1)=
_LT_TAGVAR(predeps, $1)=
_LT_TAGVAR(postdeps, $1)=
_LT_TAGVAR(compiler_lib_search_path, $1)=
X
dnl we can't use the lt_simple_compile_test_code here,
dnl because it contains code intended for an executable,
dnl not a library.  It's possible we should let each
dnl tag define a new lt_????_link_test_code variable,
dnl but it's only used here...
m4_if([$1], [], [cat > conftest.$ac_ext <<_LT_EOF
int a;
void foo (void) { a = 0; }
_LT_EOF
], [$1], [CXX], [cat > conftest.$ac_ext <<_LT_EOF
class Foo
{
public:
X  Foo (void) { a = 0; }
private:
X  int a;
};
_LT_EOF
], [$1], [F77], [cat > conftest.$ac_ext <<_LT_EOF
X      subroutine foo
X      implicit none
X      integer*4 a
X      a=0
X      return
X      end
_LT_EOF
], [$1], [FC], [cat > conftest.$ac_ext <<_LT_EOF
X      subroutine foo
X      implicit none
X      integer a
X      a=0
X      return
X      end
_LT_EOF
], [$1], [GCJ], [cat > conftest.$ac_ext <<_LT_EOF
public class foo {
X  private int a;
X  public void bar (void) {
X    a = 0;
X  }
};
_LT_EOF
])
dnl Parse the compiler output and extract the necessary
dnl objects, libraries and library flags.
if AC_TRY_EVAL(ac_compile); then
X  # Parse the compiler output and extract the necessary
X  # objects, libraries and library flags.
X
X  # Sentinel used to keep track of whether or not we are before
X  # the conftest object file.
X  pre_test_object_deps_done=no
X
X  for p in `eval "$output_verbose_link_cmd"`; do
X    case $p in
X
X    -L* | -R* | -l*)
X       # Some compilers place space between "-{L,R}" and the path.
X       # Remove the space.
X       if test $p = "-L" ||
X          test $p = "-R"; then
X	 prev=$p
X	 continue
X       else
X	 prev=
X       fi
X
X       if test "$pre_test_object_deps_done" = no; then
X	 case $p in
X	 -L* | -R*)
X	   # Internal compiler library paths should come after those
X	   # provided the user.  The postdeps already come after the
X	   # user supplied libs so there is no need to process them.
X	   if test -z "$_LT_TAGVAR(compiler_lib_search_path, $1)"; then
X	     _LT_TAGVAR(compiler_lib_search_path, $1)="${prev}${p}"
X	   else
X	     _LT_TAGVAR(compiler_lib_search_path, $1)="${_LT_TAGVAR(compiler_lib_search_path, $1)} ${prev}${p}"
X	   fi
X	   ;;
X	 # The "-l" case would never come before the object being
X	 # linked, so don't bother handling this case.
X	 esac
X       else
X	 if test -z "$_LT_TAGVAR(postdeps, $1)"; then
X	   _LT_TAGVAR(postdeps, $1)="${prev}${p}"
X	 else
X	   _LT_TAGVAR(postdeps, $1)="${_LT_TAGVAR(postdeps, $1)} ${prev}${p}"
X	 fi
X       fi
X       ;;
X
X    *.$objext)
X       # This assumes that the test object file only shows up
X       # once in the compiler output.
X       if test "$p" = "conftest.$objext"; then
X	 pre_test_object_deps_done=yes
X	 continue
X       fi
X
X       if test "$pre_test_object_deps_done" = no; then
X	 if test -z "$_LT_TAGVAR(predep_objects, $1)"; then
X	   _LT_TAGVAR(predep_objects, $1)="$p"
X	 else
X	   _LT_TAGVAR(predep_objects, $1)="$_LT_TAGVAR(predep_objects, $1) $p"
X	 fi
X       else
X	 if test -z "$_LT_TAGVAR(postdep_objects, $1)"; then
X	   _LT_TAGVAR(postdep_objects, $1)="$p"
X	 else
X	   _LT_TAGVAR(postdep_objects, $1)="$_LT_TAGVAR(postdep_objects, $1) $p"
X	 fi
X       fi
X       ;;
X
X    *) ;; # Ignore the rest.
X
X    esac
X  done
X
X  # Clean up.
X  rm -f a.out a.exe
else
X  echo "libtool.m4: error: problem compiling $1 test program"
fi
X
$RM -f confest.$objext
X
# PORTME: override above test on systems where it is broken
m4_if([$1], [CXX],
[case $host_os in
interix[[3-9]]*)
X  # Interix 3.5 installs completely hosed .la files for C++, so rather than
X  # hack all around it, let's just trust "g++" to DTRT.
X  _LT_TAGVAR(predep_objects,$1)=
X  _LT_TAGVAR(postdep_objects,$1)=
X  _LT_TAGVAR(postdeps,$1)=
X  ;;
X
linux*)
X  case `$CC -V 2>&1 | sed 5q` in
X  *Sun\ C*)
X    # Sun C++ 5.9
X
X    # The more standards-conforming stlport4 library is
X    # incompatible with the Cstd library. Avoid specifying
X    # it if it's in CXXFLAGS. Ignore libCrun as
X    # -library=stlport4 depends on it.
X    case " $CXX $CXXFLAGS " in
X    *" -library=stlport4 "*)
X      solaris_use_stlport4=yes
X      ;;
X    esac
X
X    if test "$solaris_use_stlport4" != yes; then
X      _LT_TAGVAR(postdeps,$1)='-library=Cstd -library=Crun'
X    fi
X    ;;
X  esac
X  ;;
X
solaris*)
X  case $cc_basename in
X  CC*)
X    # The more standards-conforming stlport4 library is
X    # incompatible with the Cstd library. Avoid specifying
X    # it if it's in CXXFLAGS. Ignore libCrun as
X    # -library=stlport4 depends on it.
X    case " $CXX $CXXFLAGS " in
X    *" -library=stlport4 "*)
X      solaris_use_stlport4=yes
X      ;;
X    esac
X
X    # Adding this requires a known-good setup of shared libraries for
X    # Sun compiler versions before 5.6, else PIC objects from an old
X    # archive will be linked into the output, leading to subtle bugs.
X    if test "$solaris_use_stlport4" != yes; then
X      _LT_TAGVAR(postdeps,$1)='-library=Cstd -library=Crun'
X    fi
X    ;;
X  esac
X  ;;
esac
])
X
case " $_LT_TAGVAR(postdeps, $1) " in
*" -lc "*) _LT_TAGVAR(archive_cmds_need_lc, $1)=no ;;
esac
X _LT_TAGVAR(compiler_lib_search_dirs, $1)=
if test -n "${_LT_TAGVAR(compiler_lib_search_path, $1)}"; then
X _LT_TAGVAR(compiler_lib_search_dirs, $1)=`echo " ${_LT_TAGVAR(compiler_lib_search_path, $1)}" | ${SED} -e 's! -L! !g' -e 's!^ !!'`
fi
_LT_TAGDECL([], [compiler_lib_search_dirs], [1],
X    [The directories searched by this compiler when creating a shared library])
_LT_TAGDECL([], [predep_objects], [1],
X    [Dependencies to place before and after the objects being linked to
X    create a shared library])
_LT_TAGDECL([], [postdep_objects], [1])
_LT_TAGDECL([], [predeps], [1])
_LT_TAGDECL([], [postdeps], [1])
_LT_TAGDECL([], [compiler_lib_search_path], [1],
X    [The library search path used internally by the compiler when linking
X    a shared library])
])# _LT_SYS_HIDDEN_LIBDEPS
