*'\\\[$]0 --fallback-echo"')dnl "
X  lt_ECHO=\`\$ECHO "X\$lt_ECHO" | \$Xsed -e 's/\\\\\\\\\\\\\\\[$]0 --fallback-echo"\[$]/\[$]0 --fallback-echo"/'\`
X  ;;
esac
X
_LT_OUTPUT_LIBTOOL_INIT
