X    no)  want_ofx=no ;;
X    esac[]dnl
X  ] )
AC_MSG_CHECKING(whether to use OFX)
case "${want_ofx}" in
X  yes)  AC_MSG_RESULT(yes -- failure is fatal) ;;
X  no)   AC_MSG_RESULT(no) ;;
X  auto) AC_MSG_RESULT(yes -- automatic test) ;;
esac
if test x${want_ofx} != xno ;
then
X  AC_ARG_WITH( ofx-prefix, 
X    [AS_HELP_STRING([--with-ofx-prefix=DIR],[specify where to look for libOFX])],
X    OFXPREFIX="$with_ofx_prefix" )
X
X  if test x${OFXPREFIX} != x ; then
X    LIBOFX_LIBS="-L${OFXPREFIX}/lib"
X    LIBOFX_CFLAGS="-I${OFXPREFIX}/include"
X  fi
X
X  if test "x${want_ofx}" != xno ; then
X    # Version number verified. Now check header files.
X    AC_MSG_CHECKING(for libofx/libofx.h)
X    AS_SCRUB_INCLUDE(LIBOFX_CFLAGS)
X    save_CPPFLAGS="${CPPFLAGS}"
X    CPPFLAGS="${CPPFLAGS} ${LIBOFX_CFLAGS}"
X    AC_TRY_CPP( [#include <libofx/libofx.h>], AC_MSG_RESULT(yes),
X                OFXfound=no )
X
X    if test x${OFXPREFIX} = x -a x${OFXfound} = no ; then
X      LIBOFX_LIBS="-L$/usr/local/lib"
X      LIBOFX_CFLAGS="-I$/usr/local/include"
X      AS_SCRUB_INCLUDE(LIBOFX_CFLAGS)
X      CPPFLAGS="${CPPFLAGS} ${LIBOFX_CFLAGS}"
X
X      AC_TRY_CPP( [#include <libofx/libofx.h>],
X        AC_MSG_RESULT([yes (in /usr/local)]),
X          [ if test "x${want_ofx}" = xyes ; then
X              AC_MSG_ERROR([cannot find libofx header, needed for OFX support.])
X            else
X              AC_MSG_RESULT([cannot find libofx header, needed for OFX support.])
X              want_ofx=no
X            fi
X          ] )
X    fi
X
X    if test "x${want_ofx}" != xno ; then 
X      LIBOFX_LIBS="${LIBOFX_LIBS} -lofx"
X      AC_MSG_CHECKING(for libofx)
X      save_LIBS="${LIBS}"
X      LIBS="${LIBS} ${LIBOFX_LIBS}"
X      AC_TRY_LINK( [#include <libofx/libofx.h>],
X        [ LibofxContextPtr libofx_context = libofx_get_new_context();
X          libofx_free_context(libofx_context);
X        ], AC_MSG_RESULT(yes),
X        [ if test "x${want_ofx}" = xyes ; then
X            AC_MSG_ERROR([*** Cannot compile test program for libofx library. Please check config.log for the exact error.])
X          else
X            AC_MSG_RESULT([*** Cannot compile test program for libofx library. Please check config.log for the exact error.])
X            want_ofx=no
X          fi
X        ] )
X
X      LIBS="${save_LIBS}"
X      CPPFLAGS="${save_CPPFLAGS}"
X      AC_SUBST(LIBOFX_CFLAGS)
X      AC_SUBST(LIBOFX_LIBS)
X    fi
X  fi
fi
if test "x${want_ofx}" != xno ; then
X  OFX_DIR=ofx
fi
