[AC_MSG_CHECKING([how to hardcode library paths into programs])
_LT_TAGVAR(hardcode_action, $1)=
if test -n "$_LT_TAGVAR(hardcode_libdir_flag_spec, $1)" ||
X   test -n "$_LT_TAGVAR(runpath_var, $1)" ||
X   test "X$_LT_TAGVAR(hardcode_automatic, $1)" = "Xyes" ; then
X
X  # We can hardcode non-existent directories.
X  if test "$_LT_TAGVAR(hardcode_direct, $1)" != no &&
X     # If the only mechanism to avoid hardcoding is shlibpath_var, we
X     # have to relink, otherwise we might link with an installed library
X     # when we should be linking with a yet-to-be-installed one
X     ## test "$_LT_TAGVAR(hardcode_shlibpath_var, $1)" != no &&
X     test "$_LT_TAGVAR(hardcode_minus_L, $1)" != no; then
X    # Linking always hardcodes the temporary library directory.
X    _LT_TAGVAR(hardcode_action, $1)=relink
