for var in lt_decl_all_varnames([[ \
]], lt_decl_dquote_varnames); do
X    case \`eval \\\\\$ECHO "X\\\\\$\$var"\` in
X    *[[\\\\\\\`\\"\\\$]]*)
X      eval "lt_\$var=\\\\\\"\\\`\\\$ECHO \\"X\\\$\$var\\" | \\\$Xsed -e \\"\\\$double_quote_subst\\" -e \\"\\\$sed_quote_subst\\" -e \\"\\\$delay_variable_subst\\"\\\`\\\\\\""
