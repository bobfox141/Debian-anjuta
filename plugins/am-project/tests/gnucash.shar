# LT_PATH_NM
# ----------
# find the pathname to a BSD- or MS-compatible name lister
AC_DEFUN([LT_PATH_NM],
[AC_REQUIRE([AC_PROG_CC])dnl
AC_CACHE_CHECK([for BSD- or MS-compatible name lister (nm)], lt_cv_path_NM,
[if test -n "$NM"; then
X  # Let the user override the test.
X  lt_cv_path_NM="$NM"
else
X  lt_nm_to_check="${ac_tool_prefix}nm"
X  if test -n "$ac_tool_prefix" && test "$build" = "$host"; then
X    lt_nm_to_check="$lt_nm_to_check nm"
X  fi
X  for lt_tmp_nm in $lt_nm_to_check; do
X    lt_save_ifs="$IFS"; IFS=$PATH_SEPARATOR
X    for ac_dir in $PATH /usr/ccs/bin/elf /usr/ccs/bin /usr/ucb /bin; do
X      IFS="$lt_save_ifs"
X      test -z "$ac_dir" && ac_dir=.
X      tmp_nm="$ac_dir/$lt_tmp_nm"
X      if test -f "$tmp_nm" || test -f "$tmp_nm$ac_exeext" ; then
X	# Check to see if the nm accepts a BSD-compat flag.
X	# Adding the `sed 1q' prevents false positives on HP-UX, which says:
X	#   nm: unknown option "B" ignored
X	# Tru64's nm complains that /dev/null is an invalid object file
X	case `"$tmp_nm" -B /dev/null 2>&1 | sed '1q'` in
X	*/dev/null* | *'Invalid file or object type'*)
X	  lt_cv_path_NM="$tmp_nm -B"
X	  break
X	  ;;
X	*)
X	  case `"$tmp_nm" -p /dev/null 2>&1 | sed '1q'` in
X	  */dev/null*)
X	    lt_cv_path_NM="$tmp_nm -p"
X	    break
X	    ;;
X	  *)
X	    lt_cv_path_NM=${lt_cv_path_NM="$tmp_nm"} # keep the first match, but
X	    continue # so that we can try to find one that supports BSD flags
X	    ;;
X	  esac
X	  ;;
X	esac
X      fi
X    done
X    IFS="$lt_save_ifs"
X  done
X  : ${lt_cv_path_NM=no}
fi])
if test "$lt_cv_path_NM" != "no"; then
X  NM="$lt_cv_path_NM"
else
X  # Didn't find any BSD compatible name lister, look for dumpbin.
X  AC_CHECK_TOOLS(DUMPBIN, ["dumpbin -symbols" "link -dump -symbols"], :)
X  AC_SUBST([DUMPBIN])
X  if test "$DUMPBIN" != ":"; then
X    NM="$DUMPBIN"
X  fi
fi
test -z "$NM" && NM=nm
AC_SUBST([NM])
_LT_DECL([], [NM], [1], [A BSD- or MS-compatible name lister])dnl
X
AC_CACHE_CHECK([the name lister ($NM) interface], [lt_cv_nm_interface],
X  [lt_cv_nm_interface="BSD nm"
X  echo "int some_variable = 0;" > conftest.$ac_ext
X  (eval echo "\"\$as_me:__oline__: $ac_compile\"" >&AS_MESSAGE_LOG_FD)
X  (eval "$ac_compile" 2>conftest.err)
X  cat conftest.err >&AS_MESSAGE_LOG_FD
X  (eval echo "\"\$as_me:__oline__: $NM \\\"conftest.$ac_objext\\\"\"" >&AS_MESSAGE_LOG_FD)
X  (eval "$NM \"conftest.$ac_objext\"" 2>conftest.err > conftest.out)
X  cat conftest.err >&AS_MESSAGE_LOG_FD
X  (eval echo "\"\$as_me:__oline__: output\"" >&AS_MESSAGE_LOG_FD)
X  cat conftest.out >&AS_MESSAGE_LOG_FD
X  if $GREP 'External.*some_variable' conftest.out > /dev/null; then
X    lt_cv_nm_interface="MS dumpbin"
X  fi
X  rm -f conftest*])
])# LT_PATH_NM
X
