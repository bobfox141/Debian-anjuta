# LT_CMD_MAX_LEN
#---------------
AC_DEFUN([LT_CMD_MAX_LEN],
[AC_REQUIRE([AC_CANONICAL_HOST])dnl
# find the maximum length of command line arguments
AC_MSG_CHECKING([the maximum length of command line arguments])
AC_CACHE_VAL([lt_cv_sys_max_cmd_len], [dnl
X  i=0
X  teststring="ABCD"
X
X  case $build_os in
X  msdosdjgpp*)
X    # On DJGPP, this test can blow up pretty badly due to problems in libc
X    # (any single argument exceeding 2000 bytes causes a buffer overrun
X    # during glob expansion).  Even if it were fixed, the result of this
X    # check would be larger than it should be.
X    lt_cv_sys_max_cmd_len=12288;    # 12K is about right
X    ;;
X
X  gnu*)
X    # Under GNU Hurd, this test is not required because there is
X    # no limit to the length of command line arguments.
X    # Libtool will interpret -1 as no limit whatsoever
X    lt_cv_sys_max_cmd_len=-1;
X    ;;
X
X  cygwin* | mingw* | cegcc*)
X    # On Win9x/ME, this test blows up -- it succeeds, but takes
X    # about 5 minutes as the teststring grows exponentially.
X    # Worse, since 9x/ME are not pre-emptively multitasking,
X    # you end up with a "frozen" computer, even though with patience
X    # the test eventually succeeds (with a max line length of 256k).
X    # Instead, let's just punt: use the minimum linelength reported by
X    # all of the supported platforms: 8192 (on NT/2K/XP).
X    lt_cv_sys_max_cmd_len=8192;
X    ;;
X
X  amigaos*)
X    # On AmigaOS with pdksh, this test takes hours, literally.
X    # So we just punt and use a minimum line length of 8192.
X    lt_cv_sys_max_cmd_len=8192;
X    ;;
X
X  netbsd* | freebsd* | openbsd* | darwin* | dragonfly*)
X    # This has been around since 386BSD, at least.  Likely further.
X    if test -x /sbin/sysctl; then
X      lt_cv_sys_max_cmd_len=`/sbin/sysctl -n kern.argmax`
X    elif test -x /usr/sbin/sysctl; then
X      lt_cv_sys_max_cmd_len=`/usr/sbin/sysctl -n kern.argmax`
X    else
X      lt_cv_sys_max_cmd_len=65536	# usable default for all BSDs
X    fi
X    # And add a safety zone
X    lt_cv_sys_max_cmd_len=`expr $lt_cv_sys_max_cmd_len \/ 4`
X    lt_cv_sys_max_cmd_len=`expr $lt_cv_sys_max_cmd_len \* 3`
X    ;;
X
X  interix*)
X    # We know the value 262144 and hardcode it with a safety zone (like BSD)
X    lt_cv_sys_max_cmd_len=196608
X    ;;
X
X  osf*)
X    # Dr. Hans Ekkehard Plesser reports seeing a kernel panic running configure
X    # due to this test when exec_disable_arg_limit is 1 on Tru64. It is not
X    # nice to cause kernel panics so lets avoid the loop below.
X    # First set a reasonable default.
X    lt_cv_sys_max_cmd_len=16384
X    #
X    if test -x /sbin/sysconfig; then
X      case `/sbin/sysconfig -q proc exec_disable_arg_limit` in
X        *1*) lt_cv_sys_max_cmd_len=-1 ;;
X      esac
X    fi
X    ;;
X  sco3.2v5*)
X    lt_cv_sys_max_cmd_len=102400
X    ;;
X  sysv5* | sco5v6* | sysv4.2uw2*)
X    kargmax=`grep ARG_MAX /etc/conf/cf.d/stune 2>/dev/null`
X    if test -n "$kargmax"; then
X      lt_cv_sys_max_cmd_len=`echo $kargmax | sed 's/.*[[	 ]]//'`
X    else
X      lt_cv_sys_max_cmd_len=32768
X    fi
X    ;;
X  *)
X    lt_cv_sys_max_cmd_len=`(getconf ARG_MAX) 2> /dev/null`
X    if test -n "$lt_cv_sys_max_cmd_len"; then
X      lt_cv_sys_max_cmd_len=`expr $lt_cv_sys_max_cmd_len \/ 4`
X      lt_cv_sys_max_cmd_len=`expr $lt_cv_sys_max_cmd_len \* 3`
X    else
X      # Make teststring a little bigger before we do anything with it.
X      # a 1K string should be a reasonable start.
X      for i in 1 2 3 4 5 6 7 8 ; do
X        teststring=$teststring$teststring
X      done
X      SHELL=${SHELL-${CONFIG_SHELL-/bin/sh}}
X      # If test is not a shell built-in, we'll probably end up computing a
X      # maximum length that is only half of the actual maximum length, but
X      # we can't tell.
X      while { test "X"`$SHELL [$]0 --fallback-echo "X$teststring$teststring" 2>/dev/null` \
X	         = "XX$teststring$teststring"; } >/dev/null 2>&1 &&
X	      test $i != 17 # 1/2 MB should be enough
X      do
X        i=`expr $i + 1`
X        teststring=$teststring$teststring
X      done
X      # Only check the string length outside the loop.
X      lt_cv_sys_max_cmd_len=`expr "X$teststring" : ".*" 2>&1`
X      teststring=
X      # Add a significant safety factor because C++ compilers can tack on
X      # massive amounts of additional arguments before passing them to the
X      # linker.  It appears as though 1/2 is a usable value.
X      lt_cv_sys_max_cmd_len=`expr $lt_cv_sys_max_cmd_len \/ 2`
X    fi
X    ;;
X  esac
])
if test -n $lt_cv_sys_max_cmd_len ; then
X  AC_MSG_RESULT($lt_cv_sys_max_cmd_len)
else
X  AC_MSG_RESULT(none)
fi
max_cmd_len=$lt_cv_sys_max_cmd_len
_LT_DECL([], [max_cmd_len], [0],
X    [What is the maximum length of a command?])
])# LT_CMD_MAX_LEN
X
# Old name:
AU_ALIAS([AC_LIBTOOL_SYS_MAX_CMD_LEN], [LT_CMD_MAX_LEN])
dnl aclocal-1.4 backwards compatibility:
dnl AC_DEFUN([AC_LIBTOOL_SYS_MAX_CMD_LEN], [])
X
X
