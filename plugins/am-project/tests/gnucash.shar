# LT_PATH_LD
# ----------
# find the pathname to the GNU or non-GNU linker
AC_DEFUN([LT_PATH_LD],
[AC_REQUIRE([AC_PROG_CC])dnl
AC_REQUIRE([AC_CANONICAL_HOST])dnl
AC_REQUIRE([AC_CANONICAL_BUILD])dnl
m4_require([_LT_DECL_SED])dnl
m4_require([_LT_DECL_EGREP])dnl
X
AC_ARG_WITH([gnu-ld],
X    [AS_HELP_STRING([--with-gnu-ld],
X	[assume the C compiler uses GNU ld @<:@default=no@:>@])],
X    [test "$withval" = no || with_gnu_ld=yes],
X    [with_gnu_ld=no])dnl
X
ac_prog=ld
if test "$GCC" = yes; then
X  # Check if gcc -print-prog-name=ld gives a path.
X  AC_MSG_CHECKING([for ld used by $CC])
X  case $host in
X  *-*-mingw*)
X    # gcc leaves a trailing carriage return which upsets mingw
X    ac_prog=`($CC -print-prog-name=ld) 2>&5 | tr -d '\015'` ;;
X  *)
X    ac_prog=`($CC -print-prog-name=ld) 2>&5` ;;
X  esac
X  case $ac_prog in
X    # Accept absolute paths.
X    [[\\/]]* | ?:[[\\/]]*)
X      re_direlt='/[[^/]][[^/]]*/\.\./'
X      # Canonicalize the pathname of ld
X      ac_prog=`$ECHO "$ac_prog"| $SED 's%\\\\%/%g'`
X      while $ECHO "$ac_prog" | $GREP "$re_direlt" > /dev/null 2>&1; do
X	ac_prog=`$ECHO $ac_prog| $SED "s%$re_direlt%/%"`
X      done
X      test -z "$LD" && LD="$ac_prog"
X      ;;
X  "")
X    # If it fails, then pretend we aren't using GCC.
X    ac_prog=ld
X    ;;
X  *)
X    # If it is relative, then search for the first ld in PATH.
X    with_gnu_ld=unknown
X    ;;
X  esac
elif test "$with_gnu_ld" = yes; then
X  AC_MSG_CHECKING([for GNU ld])
else
X  AC_MSG_CHECKING([for non-GNU ld])
fi
AC_CACHE_VAL(lt_cv_path_LD,
[if test -z "$LD"; then
X  lt_save_ifs="$IFS"; IFS=$PATH_SEPARATOR
X  for ac_dir in $PATH; do
X    IFS="$lt_save_ifs"
X    test -z "$ac_dir" && ac_dir=.
X    if test -f "$ac_dir/$ac_prog" || test -f "$ac_dir/$ac_prog$ac_exeext"; then
X      lt_cv_path_LD="$ac_dir/$ac_prog"
X      # Check to see if the program is GNU ld.  I'd rather use --version,
X      # but apparently some variants of GNU ld only accept -v.
X      # Break only if it was the GNU/non-GNU ld that we prefer.
X      case `"$lt_cv_path_LD" -v 2>&1 </dev/null` in
X      *GNU* | *'with BFD'*)
X	test "$with_gnu_ld" != no && break
X	;;
X      *)
X	test "$with_gnu_ld" != yes && break
X	;;
X      esac
X    fi
X  done
X  IFS="$lt_save_ifs"
else
X  lt_cv_path_LD="$LD" # Let the user override the test with a path.
fi])
LD="$lt_cv_path_LD"
if test -n "$LD"; then
X  AC_MSG_RESULT($LD)
else
X  AC_MSG_RESULT(no)
fi
test -z "$LD" && AC_MSG_ERROR([no acceptable ld found in \$PATH])
_LT_PATH_LD_GNU
AC_SUBST([LD])
X
_LT_TAGDECL([], [LD], [1], [The linker used to build libraries])
])# LT_PATH_LD
