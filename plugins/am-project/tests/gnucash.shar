X    # We can link without hardcoding, and we can hardcode nonexisting dirs.
X    _LT_TAGVAR(hardcode_action, $1)=immediate
X  fi
else
X  # We cannot hardcode anything, or else we can only hardcode existing
X  # directories.
X  _LT_TAGVAR(hardcode_action, $1)=unsupported
