# _LT_PROG_ECHO_BACKSLASH
# -----------------------
# Add some code to the start of the generated configure script which
# will find an echo command which doesn't interpret backslashes.
m4_defun([_LT_PROG_ECHO_BACKSLASH],
[_LT_SHELL_INIT([
# Check that we are running under the correct shell.
SHELL=${CONFIG_SHELL-/bin/sh}
X
case X$lt_ECHO in
XX*--fallback-echo)
X  # Remove one level of quotation (which was required for Make).
X  ECHO=`echo "$lt_ECHO" | sed 's,\\\\\[$]\\[$]0,'[$]0','`
X  ;;
esac
X
ECHO=${lt_ECHO-echo}
if test "X[$]1" = X--no-reexec; then
X  # Discard the --no-reexec flag, and continue.
X  shift
elif test "X[$]1" = X--fallback-echo; then
X  # Avoid inline document here, it may be left over
X  :
elif test "X`{ $ECHO '\t'; } 2>/dev/null`" = 'X\t' ; then
X  # Yippee, $ECHO works!
X  :
else
X  # Restart under the correct shell.
X  exec $SHELL "[$]0" --no-reexec ${1+"[$]@"}
fi
X
if test "X[$]1" = X--fallback-echo; then
X  # used as fallback echo
X  shift
X  cat <<_LT_EOF
[$]*
_LT_EOF
X  exit 0
fi
X
# The HP-UX ksh and POSIX shell print the target directory to stdout
# if CDPATH is set.
(unset CDPATH) >/dev/null 2>&1 && unset CDPATH
X
if test -z "$lt_ECHO"; then
X  if test "X${echo_test_string+set}" != Xset; then
X    # find a string as large as possible, as long as the shell can cope with it
X    for cmd in 'sed 50q "[$]0"' 'sed 20q "[$]0"' 'sed 10q "[$]0"' 'sed 2q "[$]0"' 'echo test'; do
X      # expected sizes: less than 2Kb, 1Kb, 512 bytes, 16 bytes, ...
X      if { echo_test_string=`eval $cmd`; } 2>/dev/null &&
X	 { test "X$echo_test_string" = "X$echo_test_string"; } 2>/dev/null
X      then
X        break
X      fi
X    done
X  fi
X
X  if test "X`{ $ECHO '\t'; } 2>/dev/null`" = 'X\t' &&
X     echo_testing_string=`{ $ECHO "$echo_test_string"; } 2>/dev/null` &&
X     test "X$echo_testing_string" = "X$echo_test_string"; then
X    :
X  else
X    # The Solaris, AIX, and Digital Unix default echo programs unquote
X    # backslashes.  This makes it impossible to quote backslashes using
X    #   echo "$something" | sed 's/\\/\\\\/g'
X    #
X    # So, first we look for a working echo in the user's PATH.
X
X    lt_save_ifs="$IFS"; IFS=$PATH_SEPARATOR
X    for dir in $PATH /usr/ucb; do
X      IFS="$lt_save_ifs"
X      if (test -f $dir/echo || test -f $dir/echo$ac_exeext) &&
X         test "X`($dir/echo '\t') 2>/dev/null`" = 'X\t' &&
X         echo_testing_string=`($dir/echo "$echo_test_string") 2>/dev/null` &&
X         test "X$echo_testing_string" = "X$echo_test_string"; then
X        ECHO="$dir/echo"
X        break
X      fi
X    done
X    IFS="$lt_save_ifs"
X
X    if test "X$ECHO" = Xecho; then
X      # We didn't find a better echo, so look for alternatives.
X      if test "X`{ print -r '\t'; } 2>/dev/null`" = 'X\t' &&
X         echo_testing_string=`{ print -r "$echo_test_string"; } 2>/dev/null` &&
X         test "X$echo_testing_string" = "X$echo_test_string"; then
X        # This shell has a builtin print -r that does the trick.
X        ECHO='print -r'
X      elif { test -f /bin/ksh || test -f /bin/ksh$ac_exeext; } &&
X	   test "X$CONFIG_SHELL" != X/bin/ksh; then
X        # If we have ksh, try running configure again with it.
X        ORIGINAL_CONFIG_SHELL=${CONFIG_SHELL-/bin/sh}
X        export ORIGINAL_CONFIG_SHELL
X        CONFIG_SHELL=/bin/ksh
X        export CONFIG_SHELL
X        exec $CONFIG_SHELL "[$]0" --no-reexec ${1+"[$]@"}
X      else
X        # Try using printf.
X        ECHO='printf %s\n'
X        if test "X`{ $ECHO '\t'; } 2>/dev/null`" = 'X\t' &&
X	   echo_testing_string=`{ $ECHO "$echo_test_string"; } 2>/dev/null` &&
X	   test "X$echo_testing_string" = "X$echo_test_string"; then
X	  # Cool, printf works
X	  :
X        elif echo_testing_string=`($ORIGINAL_CONFIG_SHELL "[$]0" --fallback-echo '\t') 2>/dev/null` &&
X	     test "X$echo_testing_string" = 'X\t' &&
X	     echo_testing_string=`($ORIGINAL_CONFIG_SHELL "[$]0" --fallback-echo "$echo_test_string") 2>/dev/null` &&
X	     test "X$echo_testing_string" = "X$echo_test_string"; then
X	  CONFIG_SHELL=$ORIGINAL_CONFIG_SHELL
X	  export CONFIG_SHELL
X	  SHELL="$CONFIG_SHELL"
X	  export SHELL
X	  ECHO="$CONFIG_SHELL [$]0 --fallback-echo"
X        elif echo_testing_string=`($CONFIG_SHELL "[$]0" --fallback-echo '\t') 2>/dev/null` &&
X	     test "X$echo_testing_string" = 'X\t' &&
X	     echo_testing_string=`($CONFIG_SHELL "[$]0" --fallback-echo "$echo_test_string") 2>/dev/null` &&
X	     test "X$echo_testing_string" = "X$echo_test_string"; then
X	  ECHO="$CONFIG_SHELL [$]0 --fallback-echo"
X        else
X	  # maybe with a smaller string...
X	  prev=:
X
X	  for cmd in 'echo test' 'sed 2q "[$]0"' 'sed 10q "[$]0"' 'sed 20q "[$]0"' 'sed 50q "[$]0"'; do
X	    if { test "X$echo_test_string" = "X`eval $cmd`"; } 2>/dev/null
X	    then
X	      break
X	    fi
X	    prev="$cmd"
X	  done
X
X	  if test "$prev" != 'sed 50q "[$]0"'; then
X	    echo_test_string=`eval $prev`
X	    export echo_test_string
X	    exec ${ORIGINAL_CONFIG_SHELL-${CONFIG_SHELL-/bin/sh}} "[$]0" ${1+"[$]@"}
X	  else
X	    # Oops.  We lost completely, so just stick with echo.
X	    ECHO=echo
X	  fi
X        fi
X      fi
X    fi
X  fi
fi
X
# Copy echo and quote the copy suitably for passing to libtool from
# the Makefile, instead of quoting the original, which is used later.
lt_ECHO=$ECHO
if test "X$lt_ECHO" = "X$CONFIG_SHELL [$]0 --fallback-echo"; then
X   lt_ECHO="$CONFIG_SHELL \\\$\[$]0 --fallback-echo"
fi
X
AC_SUBST(lt_ECHO)
])
_LT_DECL([], [SHELL], [1], [Shell to use when invoking shell scripts])
_LT_DECL([], [ECHO], [1],
X    [An echo program that does not interpret backslashes])
])# _LT_PROG_ECHO_BACKSLASH
X
X
