X    if ($lt_i != "" && $lt_i != ".") {
X      if ($lt_i == "..") {
X        lt_count++;
X      } else {
X        if (lt_count == 0) {
X          lt_foo="/" $lt_i lt_foo;
X        } else {
X          lt_count--;
X        }
X      }
X    }
X  }
X  if (lt_foo != "") { lt_freq[[lt_foo]]++; }
X  if (lt_freq[[lt_foo]] == 1) { print lt_foo; }
