# _LT_LANG_C_CONFIG([TAG])
# ------------------------
# Ensure that the configuration variables for a C compiler are suitably
# defined.  These variables are subsequently used by _LT_CONFIG to write
# the compiler configuration to `libtool'.
m4_defun([_LT_LANG_C_CONFIG],
[m4_require([_LT_DECL_EGREP])dnl
lt_save_CC="$CC"
AC_LANG_PUSH(C)
X
# Source file extension for C test sources.
ac_ext=c
X
# Object file extension for compiled C test sources.
objext=o
_LT_TAGVAR(objext, $1)=$objext
X
# Code to be used in simple compile tests
lt_simple_compile_test_code="int some_variable = 0;"
X
# Code to be used in simple link tests
lt_simple_link_test_code='int main(){return(0);}'
X
_LT_TAG_COMPILER
# Save the default compiler, since it gets overwritten when the other
# tags are being tested, and _LT_TAGVAR(compiler, []) is a NOP.
compiler_DEFAULT=$CC
X
# save warnings/boilerplate of simple test code
_LT_COMPILER_BOILERPLATE
_LT_LINKER_BOILERPLATE
X
## CAVEAT EMPTOR:
## There is no encapsulation within the following macros, do not change
## the running order or otherwise move them around unless you know exactly
## what you are doing...
if test -n "$compiler"; then
X  _LT_COMPILER_NO_RTTI($1)
X  _LT_COMPILER_PIC($1)
X  _LT_COMPILER_C_O($1)
X  _LT_COMPILER_FILE_LOCKS($1)
X  _LT_LINKER_SHLIBS($1)
X  _LT_SYS_DYNAMIC_LINKER($1)
X  _LT_LINKER_HARDCODE_LIBPATH($1)
X  LT_SYS_DLOPEN_SELF
X  _LT_CMD_STRIPLIB
X
X  # Report which library types will actually be built
X  AC_MSG_CHECKING([if libtool supports shared libraries])
X  AC_MSG_RESULT([$can_build_shared])
X
X  AC_MSG_CHECKING([whether to build shared libraries])
X  test "$can_build_shared" = "no" && enable_shared=no
X
X  # On AIX, shared libraries and static libraries use the same namespace, and
X  # are all built from PIC.
X  case $host_os in
X  aix3*)
X    test "$enable_shared" = yes && enable_static=no
X    if test -n "$RANLIB"; then
X      archive_cmds="$archive_cmds~\$RANLIB \$lib"
X      postinstall_cmds='$RANLIB $lib'
X    fi
X    ;;
X
X  aix[[4-9]]*)
X    if test "$host_cpu" != ia64 && test "$aix_use_runtimelinking" = no ; then
X      test "$enable_shared" = yes && enable_static=no
X    fi
X    ;;
X  esac
X  AC_MSG_RESULT([$enable_shared])
X
X  AC_MSG_CHECKING([whether to build static libraries])
X  # Make sure either enable_shared or enable_static is yes.
X  test "$enable_shared" = yes || enable_static=yes
X  AC_MSG_RESULT([$enable_static])
X
X  _LT_CONFIG($1)
fi
AC_LANG_POP
CC="$lt_save_CC"
])# _LT_LANG_C_CONFIG
