X
## We borrow guile's convention and use @-...-@ as the substitution
## brackets here, instead of the usual @...@.  This prevents autoconf
## from substituting the values directly into the left-hand sides of
## the sed substitutions.
gnc-prices.1: gnc-prices.1.in Makefile
X	rm -f $@.tmp
X	${SED} < $< > $@.tmp \
X            -e 's:@-VERSION-@:${VERSION}:g' \
X            -e 's:@-DATE-@:$(shell date +'%B %Y'):g'
X	chmod +x $@.tmp
X	mv $@.tmp $@
X
gnucash.1: gnucash.1.in Makefile
X	rm -f $@.tmp
X	${SED} < $< > $@.tmp \
X            -e 's:@-VERSION-@:${VERSION}:g' \
X            -e 's:@-DATE-@:$(shell date +'%B %Y'):g'
X	chmod +x $@.tmp
X	mv $@.tmp $@
X
tip_of_the_day.list: tip_of_the_day.list.in Makefile
X	${CC} -E -P -x c -D'N_(x)=x' -o $@.tmp $<
X	cat -s $@.tmp | ${SED} -e 's/^ *"//' \
X	                       -e 's/" *$$//' \
X	                       -e 's/"* *[|] */|/' \
X                           -e 's:@-GNUCASH_LATEST_STABLE_SERIES-@:${GNUCASH_LATEST_STABLE_SERIES}:g' > $@
X	rm -f $@.tmp
X
DISTCLEANFILES = gnc-prices.1 gnucash.1 tip_of_the_day.list
