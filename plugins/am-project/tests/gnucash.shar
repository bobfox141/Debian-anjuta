# _LT_PATH_TOOL_PREFIX(TOOL)
# --------------------------
# find a file program which can recognize shared library
AC_DEFUN([_LT_PATH_TOOL_PREFIX],
[m4_require([_LT_DECL_EGREP])dnl
AC_MSG_CHECKING([for $1])
AC_CACHE_VAL(lt_cv_path_MAGIC_CMD,
[case $MAGIC_CMD in
[[\\/*] |  ?:[\\/]*])
X  lt_cv_path_MAGIC_CMD="$MAGIC_CMD" # Let the user override the test with a path.
X  ;;
*)
X  lt_save_MAGIC_CMD="$MAGIC_CMD"
X  lt_save_ifs="$IFS"; IFS=$PATH_SEPARATOR
dnl $ac_dummy forces splitting on constant user-supplied paths.
dnl POSIX.2 word splitting is done only on the output of word expansions,
dnl not every word.  This closes a longstanding sh security hole.
X  ac_dummy="m4_if([$2], , $PATH, [$2])"
X  for ac_dir in $ac_dummy; do
X    IFS="$lt_save_ifs"
X    test -z "$ac_dir" && ac_dir=.
X    if test -f $ac_dir/$1; then
X      lt_cv_path_MAGIC_CMD="$ac_dir/$1"
X      if test -n "$file_magic_test_file"; then
X	case $deplibs_check_method in
X	"file_magic "*)
X	  file_magic_regex=`expr "$deplibs_check_method" : "file_magic \(.*\)"`
X	  MAGIC_CMD="$lt_cv_path_MAGIC_CMD"
X	  if eval $file_magic_cmd \$file_magic_test_file 2> /dev/null |
X	    $EGREP "$file_magic_regex" > /dev/null; then
X	    :
X	  else
X	    cat <<_LT_EOF 1>&2
X
*** Warning: the command libtool uses to detect shared libraries,
*** $file_magic_cmd, produces output that libtool cannot recognize.
*** The result is that libtool may fail to recognize shared libraries
*** as such.  This will affect the creation of libtool libraries that
*** depend on shared libraries, but programs linked with such libtool
*** libraries will work regardless of this problem.  Nevertheless, you
*** may want to report the problem to your system manager and/or to
*** bug-libtool@gnu.org
X
_LT_EOF
X	  fi ;;
X	esac
X      fi
X      break
X    fi
X  done
X  IFS="$lt_save_ifs"
X  MAGIC_CMD="$lt_save_MAGIC_CMD"
X  ;;
esac])
MAGIC_CMD="$lt_cv_path_MAGIC_CMD"
if test -n "$MAGIC_CMD"; then
X  AC_MSG_RESULT($MAGIC_CMD)
else
X  AC_MSG_RESULT(no)
fi
_LT_DECL([], [MAGIC_CMD], [0],
X	 [Used to examine libraries when file_magic_cmd begins with "file"])dnl
