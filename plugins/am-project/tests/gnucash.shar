m4_defun([_LT_LINKER_SHLIBS],
[AC_REQUIRE([LT_PATH_LD])dnl
AC_REQUIRE([LT_PATH_NM])dnl
m4_require([_LT_FILEUTILS_DEFAULTS])dnl
m4_require([_LT_DECL_EGREP])dnl
m4_require([_LT_DECL_SED])dnl
m4_require([_LT_CMD_GLOBAL_SYMBOLS])dnl
m4_require([_LT_TAG_COMPILER])dnl
AC_MSG_CHECKING([whether the $compiler linker ($LD) supports shared libraries])
m4_if([$1], [CXX], [
X  _LT_TAGVAR(export_symbols_cmds, $1)='$NM $libobjs $convenience | $global_symbol_pipe | $SED '\''s/.* //'\'' | sort | uniq > $export_symbols'
X  case $host_os in
X  aix[[4-9]]*)
X    # If we're using GNU nm, then we don't want the "-C" option.
X    # -C means demangle to AIX nm, but means don't demangle with GNU nm
X    if $NM -V 2>&1 | $GREP 'GNU' > /dev/null; then
X      _LT_TAGVAR(export_symbols_cmds, $1)='$NM -Bpg $libobjs $convenience | awk '\''{ if (((\$ 2 == "T") || (\$ 2 == "D") || (\$ 2 == "B")) && ([substr](\$ 3,1,1) != ".")) { print \$ 3 } }'\'' | sort -u > $export_symbols'
X    else
X      _LT_TAGVAR(export_symbols_cmds, $1)='$NM -BCpg $libobjs $convenience | awk '\''{ if (((\$ 2 == "T") || (\$ 2 == "D") || (\$ 2 == "B")) && ([substr](\$ 3,1,1) != ".")) { print \$ 3 } }'\'' | sort -u > $export_symbols'
X    fi
X    ;;
X  pw32*)
X    _LT_TAGVAR(export_symbols_cmds, $1)="$ltdll_cmds"
X  ;;
X  cygwin* | mingw* | cegcc*)
X    _LT_TAGVAR(export_symbols_cmds, $1)='$NM $libobjs $convenience | $global_symbol_pipe | $SED -e '\''/^[[BCDGRS]][[ ]]/s/.*[[ ]]\([[^ ]]*\)/\1 DATA/;/^.*[[ ]]__nm__/s/^.*[[ ]]__nm__\([[^ ]]*\)[[ ]][[^ ]]*/\1 DATA/;/^I[[ ]]/d;/^[[AITW]][[ ]]/s/.* //'\'' | sort | uniq > $export_symbols'
X  ;;
X  linux* | k*bsd*-gnu)
X    _LT_TAGVAR(link_all_deplibs, $1)=no
X  ;;
X  *)
X    _LT_TAGVAR(export_symbols_cmds, $1)='$NM $libobjs $convenience | $global_symbol_pipe | $SED '\''s/.* //'\'' | sort | uniq > $export_symbols'
X  ;;
X  esac
X  _LT_TAGVAR(exclude_expsyms, $1)=['_GLOBAL_OFFSET_TABLE_|_GLOBAL__F[ID]_.*']
], [
X  runpath_var=
X  _LT_TAGVAR(allow_undefined_flag, $1)=
X  _LT_TAGVAR(always_export_symbols, $1)=no
X  _LT_TAGVAR(archive_cmds, $1)=
X  _LT_TAGVAR(archive_expsym_cmds, $1)=
X  _LT_TAGVAR(compiler_needs_object, $1)=no
X  _LT_TAGVAR(enable_shared_with_static_runtimes, $1)=no
X  _LT_TAGVAR(export_dynamic_flag_spec, $1)=
X  _LT_TAGVAR(export_symbols_cmds, $1)='$NM $libobjs $convenience | $global_symbol_pipe | $SED '\''s/.* //'\'' | sort | uniq > $export_symbols'
X  _LT_TAGVAR(hardcode_automatic, $1)=no
X  _LT_TAGVAR(hardcode_direct, $1)=no
X  _LT_TAGVAR(hardcode_direct_absolute, $1)=no
X  _LT_TAGVAR(hardcode_libdir_flag_spec, $1)=
X  _LT_TAGVAR(hardcode_libdir_flag_spec_ld, $1)=
X  _LT_TAGVAR(hardcode_libdir_separator, $1)=
X  _LT_TAGVAR(hardcode_minus_L, $1)=no
X  _LT_TAGVAR(hardcode_shlibpath_var, $1)=unsupported
X  _LT_TAGVAR(inherit_rpath, $1)=no
X  _LT_TAGVAR(link_all_deplibs, $1)=unknown
X  _LT_TAGVAR(module_cmds, $1)=
X  _LT_TAGVAR(module_expsym_cmds, $1)=
X  _LT_TAGVAR(old_archive_from_new_cmds, $1)=
X  _LT_TAGVAR(old_archive_from_expsyms_cmds, $1)=
X  _LT_TAGVAR(thread_safe_flag_spec, $1)=
X  _LT_TAGVAR(whole_archive_flag_spec, $1)=
X  # include_expsyms should be a list of space-separated symbols to be *always*
X  # included in the symbol list
X  _LT_TAGVAR(include_expsyms, $1)=
X  # exclude_expsyms can be an extended regexp of symbols to exclude
X  # it will be wrapped by ` (' and `)$', so one must not match beginning or
X  # end of line.  Example: `a|bc|.*d.*' will exclude the symbols `a' and `bc',
X  # as well as any symbol that contains `d'.
X  _LT_TAGVAR(exclude_expsyms, $1)=['_GLOBAL_OFFSET_TABLE_|_GLOBAL__F[ID]_.*']
X  # Although _GLOBAL_OFFSET_TABLE_ is a valid symbol C name, most a.out
X  # platforms (ab)use it in PIC code, but their linkers get confused if
X  # the symbol is explicitly referenced.  Since portable code cannot
X  # rely on this symbol name, it's probably fine to never include it in
X  # preloaded symbol tables.
X  # Exclude shared library initialization/finalization symbols.
dnl Note also adjust exclude_expsyms for C++ above.
X  extract_expsyms_cmds=
X
X  case $host_os in
X  cygwin* | mingw* | pw32* | cegcc*)
X    # FIXME: the MSVC++ port hasn't been tested in a loooong time
X    # When not using gcc, we currently assume that we are using
X    # Microsoft Visual C++.
X    if test "$GCC" != yes; then
X      with_gnu_ld=no
X    fi
X    ;;
X  interix*)
X    # we just hope/assume this is gcc and not c89 (= MSVC++)
X    with_gnu_ld=yes
X    ;;
X  openbsd*)
X    with_gnu_ld=no
X    ;;
X  linux* | k*bsd*-gnu)
X    _LT_TAGVAR(link_all_deplibs, $1)=no
X    ;;
X  esac
X
X  _LT_TAGVAR(ld_shlibs, $1)=yes
X  if test "$with_gnu_ld" = yes; then
X    # If archive_cmds runs LD, not CC, wlarc should be empty
X    wlarc='${wl}'
X
X    # Set some defaults for GNU ld with shared library support. These
X    # are reset later if shared libraries are not supported. Putting them
X    # here allows them to be overridden if necessary.
X    runpath_var=LD_RUN_PATH
X    _LT_TAGVAR(hardcode_libdir_flag_spec, $1)='${wl}-rpath ${wl}$libdir'
X    _LT_TAGVAR(export_dynamic_flag_spec, $1)='${wl}--export-dynamic'
X    # ancient GNU ld didn't support --whole-archive et. al.
X    if $LD --help 2>&1 | $GREP 'no-whole-archive' > /dev/null; then
X      _LT_TAGVAR(whole_archive_flag_spec, $1)="$wlarc"'--whole-archive$convenience '"$wlarc"'--no-whole-archive'
X    else
X      _LT_TAGVAR(whole_archive_flag_spec, $1)=
X    fi
X    supports_anon_versioning=no
X    case `$LD -v 2>&1` in
X      *GNU\ gold*) supports_anon_versioning=yes ;;
X      *\ [[01]].* | *\ 2.[[0-9]].* | *\ 2.10.*) ;; # catch versions < 2.11
X      *\ 2.11.93.0.2\ *) supports_anon_versioning=yes ;; # RH7.3 ...
X      *\ 2.11.92.0.12\ *) supports_anon_versioning=yes ;; # Mandrake 8.2 ...
X      *\ 2.11.*) ;; # other 2.11 versions
X      *) supports_anon_versioning=yes ;;
X    esac
X
X    # See if GNU ld supports shared libraries.
X    case $host_os in
X    aix[[3-9]]*)
X      # On AIX/PPC, the GNU linker is very broken
X      if test "$host_cpu" != ia64; then
X	_LT_TAGVAR(ld_shlibs, $1)=no
X	cat <<_LT_EOF 1>&2
X
*** Warning: the GNU linker, at least up to release 2.9.1, is reported
*** to be unable to reliably create shared libraries on AIX.
*** Therefore, libtool is disabling shared libraries support.  If you
*** really care for shared libraries, you may want to modify your PATH
*** so that a non-GNU linker is found, and then restart.
X
_LT_EOF
X      fi
X      ;;
X
X    amigaos*)
X      case $host_cpu in
X      powerpc)
X            # see comment about AmigaOS4 .so support
X            _LT_TAGVAR(archive_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X            _LT_TAGVAR(archive_expsym_cmds, $1)=''
X        ;;
X      m68k)
X            _LT_TAGVAR(archive_cmds, $1)='$RM $output_objdir/a2ixlibrary.data~$ECHO "#define NAME $libname" > $output_objdir/a2ixlibrary.data~$ECHO "#define LIBRARY_ID 1" >> $output_objdir/a2ixlibrary.data~$ECHO "#define VERSION $major" >> $output_objdir/a2ixlibrary.data~$ECHO "#define REVISION $revision" >> $output_objdir/a2ixlibrary.data~$AR $AR_FLAGS $lib $libobjs~$RANLIB $lib~(cd $output_objdir && a2ixlibrary -32)'
X            _LT_TAGVAR(hardcode_libdir_flag_spec, $1)='-L$libdir'
X            _LT_TAGVAR(hardcode_minus_L, $1)=yes
X        ;;
X      esac
X      ;;
X
X    beos*)
X      if $LD --help 2>&1 | $GREP ': supported targets:.* elf' > /dev/null; then
X	_LT_TAGVAR(allow_undefined_flag, $1)=unsupported
X	# Joseph Beckenbach <jrb3@best.com> says some releases of gcc
X	# support --undefined.  This deserves some investigation.  FIXME
X	_LT_TAGVAR(archive_cmds, $1)='$CC -nostart $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X      else
X	_LT_TAGVAR(ld_shlibs, $1)=no
X      fi
X      ;;
X
X    cygwin* | mingw* | pw32* | cegcc*)
X      # _LT_TAGVAR(hardcode_libdir_flag_spec, $1) is actually meaningless,
X      # as there is no search path for DLLs.
X      _LT_TAGVAR(hardcode_libdir_flag_spec, $1)='-L$libdir'
X      _LT_TAGVAR(allow_undefined_flag, $1)=unsupported
X      _LT_TAGVAR(always_export_symbols, $1)=no
X      _LT_TAGVAR(enable_shared_with_static_runtimes, $1)=yes
X      _LT_TAGVAR(export_symbols_cmds, $1)='$NM $libobjs $convenience | $global_symbol_pipe | $SED -e '\''/^[[BCDGRS]][[ ]]/s/.*[[ ]]\([[^ ]]*\)/\1 DATA/'\'' | $SED -e '\''/^[[AITW]][[ ]]/s/.*[[ ]]//'\'' | sort | uniq > $export_symbols'
X
X      if $LD --help 2>&1 | $GREP 'auto-import' > /dev/null; then
X        _LT_TAGVAR(archive_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags -o $output_objdir/$soname ${wl}--enable-auto-image-base -Xlinker --out-implib -Xlinker $lib'
X	# If the export-symbols file already is a .def file (1st line
X	# is EXPORTS), use it as is; otherwise, prepend...
X	_LT_TAGVAR(archive_expsym_cmds, $1)='if test "x`$SED 1q $export_symbols`" = xEXPORTS; then
X	  cp $export_symbols $output_objdir/$soname.def;
X	else
X	  echo EXPORTS > $output_objdir/$soname.def;
X	  cat $export_symbols >> $output_objdir/$soname.def;
X	fi~
X	$CC -shared $output_objdir/$soname.def $libobjs $deplibs $compiler_flags -o $output_objdir/$soname ${wl}--enable-auto-image-base -Xlinker --out-implib -Xlinker $lib'
X      else
X	_LT_TAGVAR(ld_shlibs, $1)=no
X      fi
X      ;;
X
X    interix[[3-9]]*)
X      _LT_TAGVAR(hardcode_direct, $1)=no
X      _LT_TAGVAR(hardcode_shlibpath_var, $1)=no
X      _LT_TAGVAR(hardcode_libdir_flag_spec, $1)='${wl}-rpath,$libdir'
X      _LT_TAGVAR(export_dynamic_flag_spec, $1)='${wl}-E'
X      # Hack: On Interix 3.x, we cannot compile PIC because of a broken gcc.
X      # Instead, shared libraries are loaded at an image base (0x10000000 by
X      # default) and relocated if they conflict, which is a slow very memory
X      # consuming and fragmenting process.  To avoid this, we pick a random,
X      # 256 KiB-aligned image base between 0x50000000 and 0x6FFC0000 at link
X      # time.  Moving up from 0x10000000 also allows more sbrk(2) space.
X      _LT_TAGVAR(archive_cmds, $1)='$CC -shared $pic_flag $libobjs $deplibs $compiler_flags ${wl}-h,$soname ${wl}--image-base,`expr ${RANDOM-$$} % 4096 / 2 \* 262144 + 1342177280` -o $lib'
X      _LT_TAGVAR(archive_expsym_cmds, $1)='sed "s,^,_," $export_symbols >$output_objdir/$soname.expsym~$CC -shared $pic_flag $libobjs $deplibs $compiler_flags ${wl}-h,$soname ${wl}--retain-symbols-file,$output_objdir/$soname.expsym ${wl}--image-base,`expr ${RANDOM-$$} % 4096 / 2 \* 262144 + 1342177280` -o $lib'
X      ;;
X
X    gnu* | linux* | tpf* | k*bsd*-gnu | kopensolaris*-gnu)
X      tmp_diet=no
X      if test "$host_os" = linux-dietlibc; then
X	case $cc_basename in
X	  diet\ *) tmp_diet=yes;;	# linux-dietlibc with static linking (!diet-dyn)
X	esac
X      fi
X      if $LD --help 2>&1 | $EGREP ': supported targets:.* elf' > /dev/null \
X	 && test "$tmp_diet" = no
X      then
X	tmp_addflag=
X	tmp_sharedflag='-shared'
X	case $cc_basename,$host_cpu in
X        pgcc*)				# Portland Group C compiler
X	  _LT_TAGVAR(whole_archive_flag_spec, $1)='${wl}--whole-archive`for conv in $convenience\"\"; do test  -n \"$conv\" && new_convenience=\"$new_convenience,$conv\"; done; $ECHO \"$new_convenience\"` ${wl}--no-whole-archive'
X	  tmp_addflag=' $pic_flag'
X	  ;;
X	pgf77* | pgf90* | pgf95*)	# Portland Group f77 and f90 compilers
X	  _LT_TAGVAR(whole_archive_flag_spec, $1)='${wl}--whole-archive`for conv in $convenience\"\"; do test  -n \"$conv\" && new_convenience=\"$new_convenience,$conv\"; done; $ECHO \"$new_convenience\"` ${wl}--no-whole-archive'
X	  tmp_addflag=' $pic_flag -Mnomain' ;;
X	ecc*,ia64* | icc*,ia64*)	# Intel C compiler on ia64
X	  tmp_addflag=' -i_dynamic' ;;
X	efc*,ia64* | ifort*,ia64*)	# Intel Fortran compiler on ia64
X	  tmp_addflag=' -i_dynamic -nofor_main' ;;
X	ifc* | ifort*)			# Intel Fortran compiler
X	  tmp_addflag=' -nofor_main' ;;
X	lf95*)				# Lahey Fortran 8.1
X	  _LT_TAGVAR(whole_archive_flag_spec, $1)=
X	  tmp_sharedflag='--shared' ;;
X	xl[[cC]]*)			# IBM XL C 8.0 on PPC (deal with xlf below)
X	  tmp_sharedflag='-qmkshrobj'
X	  tmp_addflag= ;;
X	esac
X	case `$CC -V 2>&1 | sed 5q` in
X	*Sun\ C*)			# Sun C 5.9
X	  _LT_TAGVAR(whole_archive_flag_spec, $1)='${wl}--whole-archive`new_convenience=; for conv in $convenience\"\"; do test -z \"$conv\" || new_convenience=\"$new_convenience,$conv\"; done; $ECHO \"$new_convenience\"` ${wl}--no-whole-archive'
X	  _LT_TAGVAR(compiler_needs_object, $1)=yes
X	  tmp_sharedflag='-G' ;;
X	*Sun\ F*)			# Sun Fortran 8.3
X	  tmp_sharedflag='-G' ;;
X	esac
X	_LT_TAGVAR(archive_cmds, $1)='$CC '"$tmp_sharedflag""$tmp_addflag"' $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X
X        if test "x$supports_anon_versioning" = xyes; then
X          _LT_TAGVAR(archive_expsym_cmds, $1)='echo "{ global:" > $output_objdir/$libname.ver~
X	    cat $export_symbols | sed -e "s/\(.*\)/\1;/" >> $output_objdir/$libname.ver~
X	    echo "local: *; };" >> $output_objdir/$libname.ver~
X	    $CC '"$tmp_sharedflag""$tmp_addflag"' $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname ${wl}-version-script ${wl}$output_objdir/$libname.ver -o $lib'
X        fi
X
X	case $cc_basename in
X	xlf*)
X	  # IBM XL Fortran 10.1 on PPC cannot create shared libs itself
X	  _LT_TAGVAR(whole_archive_flag_spec, $1)='--whole-archive$convenience --no-whole-archive'
X	  _LT_TAGVAR(hardcode_libdir_flag_spec, $1)=
X	  _LT_TAGVAR(hardcode_libdir_flag_spec_ld, $1)='-rpath $libdir'
X	  _LT_TAGVAR(archive_cmds, $1)='$LD -shared $libobjs $deplibs $compiler_flags -soname $soname -o $lib'
X	  if test "x$supports_anon_versioning" = xyes; then
X	    _LT_TAGVAR(archive_expsym_cmds, $1)='echo "{ global:" > $output_objdir/$libname.ver~
X	      cat $export_symbols | sed -e "s/\(.*\)/\1;/" >> $output_objdir/$libname.ver~
X	      echo "local: *; };" >> $output_objdir/$libname.ver~
X	      $LD -shared $libobjs $deplibs $compiler_flags -soname $soname -version-script $output_objdir/$libname.ver -o $lib'
X	  fi
X	  ;;
X	esac
X      else
X        _LT_TAGVAR(ld_shlibs, $1)=no
X      fi
X      ;;
X
X    netbsd* | netbsdelf*-gnu)
X      if echo __ELF__ | $CC -E - | $GREP __ELF__ >/dev/null; then
X	_LT_TAGVAR(archive_cmds, $1)='$LD -Bshareable $libobjs $deplibs $linker_flags -o $lib'
X	wlarc=
X      else
X	_LT_TAGVAR(archive_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X	_LT_TAGVAR(archive_expsym_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname ${wl}-retain-symbols-file $wl$export_symbols -o $lib'
X      fi
X      ;;
X
X    solaris*)
X      if $LD -v 2>&1 | $GREP 'BFD 2\.8' > /dev/null; then
X	_LT_TAGVAR(ld_shlibs, $1)=no
X	cat <<_LT_EOF 1>&2
X
*** Warning: The releases 2.8.* of the GNU linker cannot reliably
*** create shared libraries on Solaris systems.  Therefore, libtool
*** is disabling shared libraries support.  We urge you to upgrade GNU
*** binutils to release 2.9.1 or newer.  Another option is to modify
*** your PATH or compiler configuration so that the native linker is
*** used, and then restart.
X
_LT_EOF
X      elif $LD --help 2>&1 | $GREP ': supported targets:.* elf' > /dev/null; then
X	_LT_TAGVAR(archive_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X	_LT_TAGVAR(archive_expsym_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname ${wl}-retain-symbols-file $wl$export_symbols -o $lib'
X      else
X	_LT_TAGVAR(ld_shlibs, $1)=no
X      fi
X      ;;
X
X    sysv5* | sco3.2v5* | sco5v6* | unixware* | OpenUNIX*)
X      case `$LD -v 2>&1` in
X        *\ [[01]].* | *\ 2.[[0-9]].* | *\ 2.1[[0-5]].*)
X	_LT_TAGVAR(ld_shlibs, $1)=no
X	cat <<_LT_EOF 1>&2
X
*** Warning: Releases of the GNU linker prior to 2.16.91.0.3 can not
*** reliably create shared libraries on SCO systems.  Therefore, libtool
*** is disabling shared libraries support.  We urge you to upgrade GNU
*** binutils to release 2.16.91.0.3 or newer.  Another option is to modify
*** your PATH or compiler configuration so that the native linker is
*** used, and then restart.
X
_LT_EOF
X	;;
X	*)
X	  # For security reasons, it is highly recommended that you always
X	  # use absolute paths for naming shared libraries, and exclude the
X	  # DT_RUNPATH tag from executables and libraries.  But doing so
X	  # requires that you compile everything twice, which is a pain.
X	  if $LD --help 2>&1 | $GREP ': supported targets:.* elf' > /dev/null; then
X	    _LT_TAGVAR(hardcode_libdir_flag_spec, $1)='${wl}-rpath ${wl}$libdir'
X	    _LT_TAGVAR(archive_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X	    _LT_TAGVAR(archive_expsym_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname ${wl}-retain-symbols-file $wl$export_symbols -o $lib'
X	  else
X	    _LT_TAGVAR(ld_shlibs, $1)=no
X	  fi
X	;;
X      esac
X      ;;
X
X    sunos4*)
X      _LT_TAGVAR(archive_cmds, $1)='$LD -assert pure-text -Bshareable -o $lib $libobjs $deplibs $linker_flags'
X      wlarc=
X      _LT_TAGVAR(hardcode_direct, $1)=yes
X      _LT_TAGVAR(hardcode_shlibpath_var, $1)=no
X      ;;
X
X    *)
X      if $LD --help 2>&1 | $GREP ': supported targets:.* elf' > /dev/null; then
X	_LT_TAGVAR(archive_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname -o $lib'
X	_LT_TAGVAR(archive_expsym_cmds, $1)='$CC -shared $libobjs $deplibs $compiler_flags ${wl}-soname $wl$soname ${wl}-retain-symbols-file $wl$export_symbols -o $lib'
X      else
X	_LT_TAGVAR(ld_shlibs, $1)=no
X      fi
X      ;;
X    esac
X
X    if test "$_LT_TAGVAR(ld_shlibs, $1)" = no; then
X      runpath_var=
X      _LT_TAGVAR(hardcode_libdir_flag_spec, $1)=
X      _LT_TAGVAR(export_dynamic_flag_spec, $1)=
X      _LT_TAGVAR(whole_archive_flag_spec, $1)=
X    fi
X  else
X    # PORTME fill in a description of your system's linker (not GNU ld)
X    case $host_os in
X    aix3*)
X      _LT_TAGVAR(allow_undefined_flag, $1)=unsupported
X      _LT_TAGVAR(always_export_symbols, $1)=yes
X      _LT_TAGVAR(archive_expsym_cmds, $1)='$LD -o $output_objdir/$soname $libobjs $deplibs $linker_flags -bE:$export_symbols -T512 -H512 -bM:SRE~$AR $AR_FLAGS $lib $output_objdir/$soname'
X      # Note: this linker hardcodes the directories in LIBPATH if there
X      # are no directories specified by -L.
X      _LT_TAGVAR(hardcode_minus_L, $1)=yes
X      if test "$GCC" = yes && test -z "$lt_prog_compiler_static"; then
X	# Neither direct hardcoding nor static linking is supported with a
X	# broken collect2.
X	_LT_TAGVAR(hardcode_direct, $1)=unsupported
X      fi
X      ;;
X
X    aix[[4-9]]*)
X      if test "$host_cpu" = ia64; then
X	# On IA64, the linker does run time linking by default, so we don't
X	# have to do anything special.
X	aix_use_runtimelinking=no
X	exp_sym_flag='-Bexport'
X	no_entry_flag=""
X      else
X	# If we're using GNU nm, then we don't want the "-C" option.
X	# -C means demangle to AIX nm, but means don't demangle with GNU nm
X	if $NM -V 2>&1 | $GREP 'GNU' > /dev/null; then
