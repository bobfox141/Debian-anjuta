X
mingw* | pw32*)
X  # Base MSYS/MinGW do not provide the 'file' command needed by
X  # func_win32_libid shell function, so use a weaker test based on 'objdump',
X  # unless we find 'file', for example because we are cross-compiling.
X  if ( file / ) >/dev/null 2>&1; then
X    lt_cv_deplibs_check_method='file_magic ^x86 archive import|^x86 DLL'
X    lt_cv_file_magic_cmd='func_win32_libid'
X  else
X    lt_cv_deplibs_check_method='file_magic file format pei*-i386(.*architecture: i386)?'
X    lt_cv_file_magic_cmd='$OBJDUMP -f'
X  fi
X  ;;
X
cegcc)
X  # use the weaker test based on 'objdump'. See mingw*.
X  lt_cv_deplibs_check_method='file_magic file format pe-arm-.*little(.*architecture: arm)?'
X  lt_cv_file_magic_cmd='$OBJDUMP -f'
X  ;;
X
darwin* | rhapsody*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
freebsd* | dragonfly*)
X  if echo __ELF__ | $CC -E - | $GREP __ELF__ > /dev/null; then
X    case $host_cpu in
X    i*86 )
X      # Not sure whether the presence of OpenBSD here was a mistake.
X      # Let's accept both of them until this is cleared up.
X      lt_cv_deplibs_check_method='file_magic (FreeBSD|OpenBSD|DragonFly)/i[[3-9]]86 (compact )?demand paged shared library'
X      lt_cv_file_magic_cmd=/usr/bin/file
X      lt_cv_file_magic_test_file=`echo /usr/lib/libc.so.*`
X      ;;
X    esac
X  else
X    lt_cv_deplibs_check_method=pass_all
X  fi
X  ;;
X
gnu*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
hpux10.20* | hpux11*)
X  lt_cv_file_magic_cmd=/usr/bin/file
X  case $host_cpu in
X  ia64*)
X    lt_cv_deplibs_check_method='file_magic (s[[0-9]][[0-9]][[0-9]]|ELF-[[0-9]][[0-9]]) shared object file - IA64'
X    lt_cv_file_magic_test_file=/usr/lib/hpux32/libc.so
X    ;;
X  hppa*64*)
X    [lt_cv_deplibs_check_method='file_magic (s[0-9][0-9][0-9]|ELF-[0-9][0-9]) shared object file - PA-RISC [0-9].[0-9]']
X    lt_cv_file_magic_test_file=/usr/lib/pa20_64/libc.sl
X    ;;
X  *)
X    lt_cv_deplibs_check_method='file_magic (s[[0-9]][[0-9]][[0-9]]|PA-RISC[[0-9]].[[0-9]]) shared library'
X    lt_cv_file_magic_test_file=/usr/lib/libc.sl
X    ;;
X  esac
X  ;;
X
interix[[3-9]]*)
X  # PIC code is broken on Interix 3.x, that's why |\.a not |_pic\.a here
X  lt_cv_deplibs_check_method='match_pattern /lib[[^/]]+(\.so|\.a)$'
X  ;;
X
irix5* | irix6* | nonstopux*)
X  case $LD in
X  *-32|*"-32 ") libmagic=32-bit;;
X  *-n32|*"-n32 ") libmagic=N32;;
X  *-64|*"-64 ") libmagic=64-bit;;
X  *) libmagic=never-match;;
X  esac
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
# This must be Linux ELF.
linux* | k*bsd*-gnu | kopensolaris*-gnu)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
netbsd* | netbsdelf*-gnu)
X  if echo __ELF__ | $CC -E - | $GREP __ELF__ > /dev/null; then
X    lt_cv_deplibs_check_method='match_pattern /lib[[^/]]+(\.so\.[[0-9]]+\.[[0-9]]+|_pic\.a)$'
X  else
X    lt_cv_deplibs_check_method='match_pattern /lib[[^/]]+(\.so|_pic\.a)$'
X  fi
X  ;;
X
newos6*)
X  lt_cv_deplibs_check_method='file_magic ELF [[0-9]][[0-9]]*-bit [[ML]]SB (executable|dynamic lib)'
X  lt_cv_file_magic_cmd=/usr/bin/file
X  lt_cv_file_magic_test_file=/usr/lib/libnls.so
X  ;;
X
*nto* | *qnx*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
openbsd*)
X  if test -z "`echo __ELF__ | $CC -E - | $GREP __ELF__`" || test "$host_os-$host_cpu" = "openbsd2.8-powerpc"; then
X    lt_cv_deplibs_check_method='match_pattern /lib[[^/]]+(\.so\.[[0-9]]+\.[[0-9]]+|\.so|_pic\.a)$'
X  else
X    lt_cv_deplibs_check_method='match_pattern /lib[[^/]]+(\.so\.[[0-9]]+\.[[0-9]]+|_pic\.a)$'
X  fi
X  ;;
X
osf3* | osf4* | osf5*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
rdos*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
solaris*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
sysv5* | sco3.2v5* | sco5v6* | unixware* | OpenUNIX* | sysv4*uw2*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
X
sysv4 | sysv4.3*)
X  case $host_vendor in
X  motorola)
X    lt_cv_deplibs_check_method='file_magic ELF [[0-9]][[0-9]]*-bit [[ML]]SB (shared object|dynamic lib) M[[0-9]][[0-9]]* Version [[0-9]]'
X    lt_cv_file_magic_test_file=`echo /usr/lib/libc.so*`
X    ;;
X  ncr)
X    lt_cv_deplibs_check_method=pass_all
X    ;;
X  sequent)
X    lt_cv_file_magic_cmd='/bin/file'
X    lt_cv_deplibs_check_method='file_magic ELF [[0-9]][[0-9]]*-bit [[LM]]SB (shared object|dynamic lib )'
X    ;;
X  sni)
X    lt_cv_file_magic_cmd='/bin/file'
X    lt_cv_deplibs_check_method="file_magic ELF [[0-9]][[0-9]]*-bit [[LM]]SB dynamic lib"
X    lt_cv_file_magic_test_file=/lib/libc.so
X    ;;
X  siemens)
X    lt_cv_deplibs_check_method=pass_all
X    ;;
X  pc)
X    lt_cv_deplibs_check_method=pass_all
X    ;;
X  esac
X  ;;
X
tpf*)
X  lt_cv_deplibs_check_method=pass_all
X  ;;
esac
])
file_magic_cmd=$lt_cv_file_magic_cmd
deplibs_check_method=$lt_cv_deplibs_check_method
test -z "$deplibs_check_method" && deplibs_check_method=unknown
X
_LT_DECL([], [deplibs_check_method], [1],
X    [Method to check whether dependent libraries are shared objects])
_LT_DECL([], [file_magic_cmd], [1],
X    [Command to use when deplibs_check_method == "file_magic"])
])# _LT_CHECK_MAGIC_METHOD
