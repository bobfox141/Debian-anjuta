# _LT_CMD_GLOBAL_SYMBOLS
# ----------------------
m4_defun([_LT_CMD_GLOBAL_SYMBOLS],
[AC_REQUIRE([AC_CANONICAL_HOST])dnl
AC_REQUIRE([AC_PROG_CC])dnl
AC_REQUIRE([LT_PATH_NM])dnl
AC_REQUIRE([LT_PATH_LD])dnl
m4_require([_LT_DECL_SED])dnl
m4_require([_LT_DECL_EGREP])dnl
m4_require([_LT_TAG_COMPILER])dnl
X
# Check for command to grab the raw symbol name followed by C symbol from nm.
AC_MSG_CHECKING([command to parse $NM output from $compiler object])
AC_CACHE_VAL([lt_cv_sys_global_symbol_pipe],
[
# These are sane defaults that work on at least a few old systems.
# [They come from Ultrix.  What could be older than Ultrix?!! ;)]
X
# Character class describing NM global symbol codes.
symcode='[[BCDEGRST]]'
X
# Regexp to match symbols that can be accessed directly from C.
sympat='\([[_A-Za-z]][[_A-Za-z0-9]]*\)'
X
# Define system-specific variables.
case $host_os in
aix*)
X  symcode='[[BCDT]]'
X  ;;
cygwin* | mingw* | pw32* | cegcc*)
X  symcode='[[ABCDGISTW]]'
X  ;;
hpux*)
X  if test "$host_cpu" = ia64; then
X    symcode='[[ABCDEGRST]]'
X  fi
X  ;;
irix* | nonstopux*)
X  symcode='[[BCDEGRST]]'
X  ;;
osf*)
X  symcode='[[BCDEGQRST]]'
X  ;;
solaris*)
X  symcode='[[BDRT]]'
X  ;;
sco3.2v5*)
X  symcode='[[DT]]'
X  ;;
sysv4.2uw2*)
X  symcode='[[DT]]'
X  ;;
sysv5* | sco5v6* | unixware* | OpenUNIX*)
X  symcode='[[ABDT]]'
X  ;;
sysv4)
X  symcode='[[DFNSTU]]'
X  ;;
esac
X
# If we're using GNU nm, then use its standard symbol codes.
case `$NM -V 2>&1` in
*GNU* | *'with BFD'*)
X  symcode='[[ABCDGIRSTW]]' ;;
esac
X
# Transform an extracted symbol line into a proper C declaration.
# Some systems (esp. on ia64) link data and code symbols differently,
# so use this general approach.
lt_cv_sys_global_symbol_to_cdecl="sed -n -e 's/^T .* \(.*\)$/extern int \1();/p' -e 's/^$symcode* .* \(.*\)$/extern char \1;/p'"
X
# Transform an extracted symbol line into symbol name and symbol address
lt_cv_sys_global_symbol_to_c_name_address="sed -n -e 's/^: \([[^ ]]*\) $/  {\\\"\1\\\", (void *) 0},/p' -e 's/^$symcode* \([[^ ]]*\) \([[^ ]]*\)$/  {\"\2\", (void *) \&\2},/p'"
lt_cv_sys_global_symbol_to_c_name_address_lib_prefix="sed -n -e 's/^: \([[^ ]]*\) $/  {\\\"\1\\\", (void *) 0},/p' -e 's/^$symcode* \([[^ ]]*\) \(lib[[^ ]]*\)$/  {\"\2\", (void *) \&\2},/p' -e 's/^$symcode* \([[^ ]]*\) \([[^ ]]*\)$/  {\"lib\2\", (void *) \&\2},/p'"
X
# Handle CRLF in mingw tool chain
opt_cr=
case $build_os in
mingw*)
X  opt_cr=`$ECHO 'x\{0,1\}' | tr x '\015'` # option cr in regexp
X  ;;
esac
X
# Try without a prefix underscore, then with it.
for ac_symprfx in "" "_"; do
X
X  # Transform symcode, sympat, and symprfx into a raw symbol and a C symbol.
X  symxfrm="\\1 $ac_symprfx\\2 \\2"
X
X  # Write the raw and C identifiers.
X  if test "$lt_cv_nm_interface" = "MS dumpbin"; then
X    # Fake it for dumpbin and say T for any non-static function
X    # and D for any global variable.
X    # Also find C++ and __fastcall symbols from MSVC++,
X    # which start with @ or ?.
X    lt_cv_sys_global_symbol_pipe="$AWK ['"\
"     {last_section=section; section=\$ 3};"\
"     /Section length .*#relocs.*(pick any)/{hide[last_section]=1};"\
"     \$ 0!~/External *\|/{next};"\
"     / 0+ UNDEF /{next}; / UNDEF \([^|]\)*()/{next};"\
"     {if(hide[section]) next};"\
"     {f=0}; \$ 0~/\(\).*\|/{f=1}; {printf f ? \"T \" : \"D \"};"\
"     {split(\$ 0, a, /\||\r/); split(a[2], s)};"\
"     s[1]~/^[@?]/{print s[1], s[1]; next};"\
"     s[1]~prfx {split(s[1],t,\"@\"); print t[1], substr(t[1],length(prfx))}"\
"     ' prfx=^$ac_symprfx]"
X  else
X    lt_cv_sys_global_symbol_pipe="sed -n -e 's/^.*[[	 ]]\($symcode$symcode*\)[[	 ]][[	 ]]*$ac_symprfx$sympat$opt_cr$/$symxfrm/p'"
X  fi
X
X  # Check to see that the pipe works correctly.
X  pipe_works=no
X
X  rm -f conftest*
X  cat > conftest.$ac_ext <<_LT_EOF
#ifdef __cplusplus
extern "C" {
#endif
char nm_test_var;
void nm_test_func(void);
void nm_test_func(void){}
#ifdef __cplusplus
}
#endif
int main(){nm_test_var='a';nm_test_func();return(0);}
_LT_EOF
X
X  if AC_TRY_EVAL(ac_compile); then
X    # Now try to grab the symbols.
X    nlist=conftest.nm
X    if AC_TRY_EVAL(NM conftest.$ac_objext \| $lt_cv_sys_global_symbol_pipe \> $nlist) && test -s "$nlist"; then
X      # Try sorting and uniquifying the output.
X      if sort "$nlist" | uniq > "$nlist"T; then
X	mv -f "$nlist"T "$nlist"
X      else
X	rm -f "$nlist"T
X      fi
X
X      # Make sure that we snagged all the symbols we need.
X      if $GREP ' nm_test_var$' "$nlist" >/dev/null; then
X	if $GREP ' nm_test_func$' "$nlist" >/dev/null; then
X	  cat <<_LT_EOF > conftest.$ac_ext
#ifdef __cplusplus
extern "C" {
#endif
X
_LT_EOF
X	  # Now generate the symbol file.
X	  eval "$lt_cv_sys_global_symbol_to_cdecl"' < "$nlist" | $GREP -v main >> conftest.$ac_ext'
X
X	  cat <<_LT_EOF >> conftest.$ac_ext
X
/* The mapping between symbol names and symbols.  */
const struct {
X  const char *name;
X  void       *address;
}
lt__PROGRAM__LTX_preloaded_symbols[[]] =
{
X  { "@PROGRAM@", (void *) 0 },
_LT_EOF
X	  $SED "s/^$symcode$symcode* \(.*\) \(.*\)$/  {\"\2\", (void *) \&\2},/" < "$nlist" | $GREP -v main >> conftest.$ac_ext
X	  cat <<\_LT_EOF >> conftest.$ac_ext
X  {0, (void *) 0}
};
X
/* This works around a problem in FreeBSD linker */
#ifdef FREEBSD_WORKAROUND
static const void *lt_preloaded_setup() {
X  return lt__PROGRAM__LTX_preloaded_symbols;
}
#endif
X
#ifdef __cplusplus
}
#endif
_LT_EOF
X	  # Now try linking the two files.
X	  mv conftest.$ac_objext conftstm.$ac_objext
X	  lt_save_LIBS="$LIBS"
X	  lt_save_CFLAGS="$CFLAGS"
X	  LIBS="conftstm.$ac_objext"
X	  CFLAGS="$CFLAGS$_LT_TAGVAR(lt_prog_compiler_no_builtin_flag, $1)"
X	  if AC_TRY_EVAL(ac_link) && test -s conftest${ac_exeext}; then
X	    pipe_works=yes
X	  fi
X	  LIBS="$lt_save_LIBS"
X	  CFLAGS="$lt_save_CFLAGS"
X	else
X	  echo "cannot find nm_test_func in $nlist" >&AS_MESSAGE_LOG_FD
X	fi
X      else
X	echo "cannot find nm_test_var in $nlist" >&AS_MESSAGE_LOG_FD
X      fi
X    else
X      echo "cannot run $lt_cv_sys_global_symbol_pipe" >&AS_MESSAGE_LOG_FD
X    fi
X  else
X    echo "$progname: failed program was:" >&AS_MESSAGE_LOG_FD
X    cat conftest.$ac_ext >&5
X  fi
X  rm -rf conftest* conftst*
X
X  # Do not use the global_symbol_pipe unless it works.
X  if test "$pipe_works" = yes; then
X    break
X  else
X    lt_cv_sys_global_symbol_pipe=
X  fi
done
])
if test -z "$lt_cv_sys_global_symbol_pipe"; then
X  lt_cv_sys_global_symbol_to_cdecl=
fi
if test -z "$lt_cv_sys_global_symbol_pipe$lt_cv_sys_global_symbol_to_cdecl"; then
X  AC_MSG_RESULT(failed)
else
X  AC_MSG_RESULT(ok)
fi
X
_LT_DECL([global_symbol_pipe], [lt_cv_sys_global_symbol_pipe], [1],
X    [Take the output of nm and produce a listing of raw symbols and C names])
_LT_DECL([global_symbol_to_cdecl], [lt_cv_sys_global_symbol_to_cdecl], [1],
X    [Transform the output of nm in a proper C declaration])
_LT_DECL([global_symbol_to_c_name_address],
X    [lt_cv_sys_global_symbol_to_c_name_address], [1],
X    [Transform the output of nm in a C name address pair])
_LT_DECL([global_symbol_to_c_name_address_lib_prefix],
X    [lt_cv_sys_global_symbol_to_c_name_address_lib_prefix], [1],
X    [Transform the output of nm in a C name address pair when lib prefix is needed])
]) # _LT_CMD_GLOBAL_SYMBOLS
X
X
# _LT_COMPILER_PIC([TAGNAME])
# ---------------------------
m4_defun([_LT_COMPILER_PIC],
[m4_require([_LT_TAG_COMPILER])dnl
_LT_TAGVAR(lt_prog_compiler_wl, $1)=
_LT_TAGVAR(lt_prog_compiler_pic, $1)=
_LT_TAGVAR(lt_prog_compiler_static, $1)=
X
AC_MSG_CHECKING([for $compiler option to produce PIC])
m4_if([$1], [CXX], [
X  # C++ specific cases for pic, static, wl, etc.
X  if test "$GXX" = yes; then
X    _LT_TAGVAR(lt_prog_compiler_wl, $1)='-Wl,'
X    _LT_TAGVAR(lt_prog_compiler_static, $1)='-static'
X
X    case $host_os in
X    aix*)
X      # All AIX code is PIC.
X      if test "$host_cpu" = ia64; then
X	# AIX 5 now supports IA64 processor
X	_LT_TAGVAR(lt_prog_compiler_static, $1)='-Bstatic'
X      fi
X      ;;
X
X    amigaos*)
X      case $host_cpu in
X      powerpc)
X            # see comment about AmigaOS4 .so support
X            _LT_TAGVAR(lt_prog_compiler_pic, $1)='-fPIC'
X        ;;
X      m68k)
X            # FIXME: we need at least 68020 code to build shared libraries, but
X            # adding the `-m68020' flag to GCC prevents building anything better,
X            # like `-m68040'.
X            _LT_TAGVAR(lt_prog_compiler_pic, $1)='-m68020 -resident32 -malways-restore-a4'
X        ;;
X      esac
X      ;;
X
X    beos* | irix5* | irix6* | nonstopux* | osf3* | osf4* | osf5*)
X      # PIC is the default for these OSes.
X      ;;
